{
  "name": "multi-tenant-base-project",
  "description": "A base project for multi-tenant applications using Next.js, Drizzle ORM, and Postgres.",
  "version": "0.1.1",
  "private": true,
  "author": {
    "name": "muutaa",
    "email": "contact@muutaa.com",
    "url": "https://muutaa.com",
    "organization": "Muutaa Inc."
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "admin:migration:generate": "drizzle-kit generate:pg --config drizzle.config.admin.ts",
    "admin:migration:up": "drizzle-kit up:pg --config drizzle.config.admin.ts",
    "admin:migration:check": "drizzle-kit up:pg --config drizzle.config.admin.ts",
    "admin:migration:generate:custom": "drizzle-kit generate:pg --custom --config drizzle.config.admin.ts",
    "admin:migration:drop": "drizzle-kit drop --config drizzle.config.admin.ts",
    "admin:migration:deploy": "tsx lib/deploy/deploy.admin.ts",
    "admin:push": "drizzle-kit push:pg --config drizzle.config.admin.ts",
    "admin:introspect": "drizzle-kit introspect:pg --config drizzle.config.admin.ts",
    "admin:studio": "drizzle-kit studio --config drizzle.config.admin.ts --host 127.0.0.1",
    "tenant:migration:generate": "drizzle-kit generate:pg --config drizzle.config.tenant.ts",
    "tenant:migration:up": "drizzle-kit up:pg --config drizzle.config.tenant.ts",
    "tenant:migration:check": "drizzle-kit up:pg --config drizzle.config.tenant.ts",
    "tenant:migration:generate:custom": "drizzle-kit generate:pg --custom --config drizzle.config.tenant.ts",
    "tenant:migration:drop": "drizzle-kit drop --config drizzle.config.tenant.ts",
    "tenant:migration:deploy": "tsx lib/deploy/deploy.tenant.ts",
    "tenant:push": "drizzle-kit push:pg --config drizzle.config.tenant.ts",
    "tenant:introspect": "drizzle-kit introspect:pg --config drizzle.config.tenant.ts",
    "tenant:studio": "drizzle-kit studio --config drizzle.config.tenant.ts --host 127.0.0.1",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "next": "15.1.2",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.17.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8",
    "prettier": "3.4.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "typescript-eslint": "^8.19.0"
  }
}
